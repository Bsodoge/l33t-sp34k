{
    "manifest_version" : 3,
    "name": "l33t sp34k",
    "version": "1.0",

    "description": "Converts normal text to l33t sp34k.",
    "homepage_url": "https://github.com/Bsodoge/l33t-sp34k",
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "64": "icons/icon64.png"
    },
    "content_scripts": [
        {
            "matches":["<all_urls>"],
            "js": ["contentScript.js"]
        }
    ],
    "action": {
        "browser_style": true,
        "default_title": "l33t sp34k"
    }  
}